{
  "name": "@amzn/address-form-sdk-js",
  "description": "Address Form SDK for Javascript",
  "version": "1.0.0",
  "main": "./dist/lib/address-form-sdk.cjs.js",
  "types": "dist/lib/main.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "lint": "eslint . && prettier --check .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build": "npm run build:lib && npm run build:standalone",
    "build:lib": "tsc -b && vite build",
    "build:standalone": "tsc -b && vite build --config ./vite.config.standalone.ts",
    "build:storybook": "storybook build",
    "test": "tsc -b && eslint . && vitest run --coverage",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.15.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@vanilla-extract/vite-plugin": "^5.0.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.3",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.12.0",
    "glob": "^10.4.5",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "jsdom-testing-mocks": "^1.13.1",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "storybook": "^8.6.12",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.0"
  },
  "dependencies": {
    "@aws-sdk/client-geo-places": "^3.799.0",
    "@aws/amazon-location-utilities-auth-helper": "^1.2.1",
    "@headlessui/react": "^2.2.2",
    "@tanstack/react-query": "^5.81.5",
    "@vanilla-extract/css": "^1.17.1",
    "clsx": "^2.1.1",
    "maplibre-gl": "^5.4.0",
    "react-hook-form": "^7.57.0",
    "react-map-gl": "^8.0.4"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
